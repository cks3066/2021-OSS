{
  "name": "2021-oos",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "nodemon",
    "build": "npm run build:server && npm run build:assets",
    "build:assets": "webpack --mode=production --config webpack.config.js",
    "build:server": "babel src -d build",
    "tailwind:build": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind_style.css",
    "dev:assets": "webpack --mode=development -w --config webpack.config.js"
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "axios": "^0.24.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.19.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "firebase": "^9.0.2",
    "nodemon": "^2.0.12",
    "pug": "^3.0.2",
    "quill": "1.3.6",
    "quill-image-resize-module": "^3.0.0",
    "regenerator-runtime": "^0.13.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.8",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.15",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  }
}
